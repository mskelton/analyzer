{
  "name": "analyzer",
  "description": "Forex trading analysis and sharing tool.",
  "author": "Mark Skelton",
  "license": "ISC",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "build": "yarn build:css --minify && remix build",
    "dev:app": "remix dev",
    "dev:css": "yarn build:css --watch",
    "dev": "concurrently -n frontend,css -c 'blue.bold,magenta.bold' 'yarn dev:app' 'yarn dev:css'",
    "format": "prettier --write .",
    "lint": "eslint .",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "test": "playwright test",
    "ts": "tsc -b"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "bcrypt": "5.0.0",
    "clsx": "^1.1.1",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@mskelton/eslint-config": "^7.1.0",
    "@mskelton/tsconfig": "^1.1.1",
    "@playwright/test": "^1.17.2",
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/forms": "^0.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-playwright": "^0.7.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort": "^2.3.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.4"
  }
}
